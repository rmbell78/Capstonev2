<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>


<AnchorPane prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.capstone.controllers.StartController">
   <children>
      <VBox alignment="CENTER" prefHeight="500.0" prefWidth="500.0" spacing="15.0">
         <children>
            <Button fx:id="start_Menu_Start_Game" mnemonicParsing="false" onAction="#onStartGameButtonClicked" text="Start Game" />
            <Button fx:id="start_Menu_Information" mnemonicParsing="false" onAction="#onInformationButtonClicked" text="Information" />
            <Button fx:id="start_Menu_Exit" mnemonicParsing="false" onAction="#onExitButtonClicked" text="Exit" />
         </children>
      </VBox>
   </children>
</AnchorPane>
